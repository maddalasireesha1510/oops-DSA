#include<bits/stdc++.h>
using namespace std;
class Stack()
{
    int *arr;
    int cat;
    int top;
    public:
    Stack(int c)
    {
        cap=c;
        arr=new int[cap];
        top=-1;
    }
    void push(int data)
    {
        if(top==cap-1)
        {
            cout<<"stack is full";
            return;
        }
        top++;
        arr[top]=data;
    }
    int pop()
    {
        if(top==-1)
        {
            cout<<"stack is empty";
            return -1;
        }
        top--;
        return arr[top+1];
    }
    int peek()
    {
        if(top==-1)
        {
            cout<<"stack is empty";
            return -1;
        }
        return arr[top];
    }
    bool isempty()
    {
        return top==-1;
    }
    int size()
    {
        return top+1;
    } 

};
int main()
{
    Stack mystack(5);
    mystack.push(10);
    mystack.push(20);
    mystack.push(30);
    cout<<mystack.size()<<"/n";
    cout<<mystack.peek()<<"/n";
    cout<<mystack.pop()<<"/n";
    cout<<mystack.peek()<<"/n";
}
